        <servlet-class>Servlets.CrearModificarEmpleado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarAuto</servlet-name>
        <servlet-class>Servlets.AgregarAuto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarEmpleados</servlet-name>
        <servlet-class>Servlets.EliminarEmpleados</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearModificarProducto</servlet-name>
        <servlet-class>Servlets.CrearModificarProducto</servlet-class>
    </servlet>
