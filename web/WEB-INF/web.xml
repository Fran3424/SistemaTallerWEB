    </servlet>
    <servlet>
        <servlet-name>RedireccionaAuto</servlet-name>
        <servlet-class>Servlets.RedireccionaAuto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarCarrito</servlet-name>
        <servlet-class>Servlets.GuardarCarrito</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinalizarCompra</servlet-name>
        <servlet-class>Servlets.FinalizarCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarOrdenCompra</servlet-name>
        <servlet-class>Servlets.EliminarOrdenCompra</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CrearModificarCliente</servlet-name>
        <url-pattern>/CrearModificarCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearModificarEmpleado</servlet-name>
        <url-pattern>/CrearModificarEmpleado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarAuto</servlet-name>
        <url-pattern>/AgregarAuto</url-pattern>
    </servlet-mapping>
