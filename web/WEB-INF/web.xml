    <servlet-mapping>
        <servlet-name>EliminarOrdenCompra</servlet-name>
        <url-pattern>/EliminarOrdenCompra</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
