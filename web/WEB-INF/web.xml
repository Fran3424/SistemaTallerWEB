        <servlet-name>EliminarEmpleados</servlet-name>
        <url-pattern>/EliminarEmpleados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearModificarProducto</servlet-name>
        <url-pattern>/CrearModificarProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarProducto</servlet-name>
        <url-pattern>/EliminarProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
